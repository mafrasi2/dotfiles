URxvt.saveLines: 1000
URxvt.scrollBar: false
URxvt.foreground: #E0D8D8
URxvt.background: #002B36
URxvt.secondaryScroll: true
!URxvt.font: xft:DejaVu Sans Mono for Powerline:size=14
!URxvt.font: xft:Inconsolata:size=17
!, xft:Roboto Mono Thin Italic for Powerline, xft:Literation Mono Powerline, xft:Monofur for Powerline
urxvt*font: xft:DejaVu Sans Mono:style=Book:antialias=true:size=14, \
            xft:FreeSerif:style=Regular:size=14, \
            xft:unifont:style=Medium:antialias=false:size=14 \
            xft:Symbola:style=Regular:size=14 \
            xft:Inconsolata:style=Regular:size=14
URxvt.perl-ext-common: default,matcher,background
URxvt.urlLauncher: firefox
URxvt.matcher.button: 1
URxvt.tabbed.secondaryScroll: true
!URxvt.tabbed.font: xft:DejaVu Sans Mono for Powerline Book:pixelsize=14
!,xft:Inconsolata for Powerline, xft:Roboto Mono Thin Italic for Powerline, xft:Literation Mono Powerline, xft:Monofur for Powerline

URxvt.letterSpace: 0

